# Update these with your Supabase details from your project settings > API
# https://app.supabase.com/project/_/settings/api
NEXT_PUBLIC_SUPABASE_URL=your-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Configuración de Square
# Para tu entorno de desarrollo, usa el token de sandbox
SQUARE_ACCESS_TOKEN=tu_token_de_sandbox

# Para copiar productos de producción a sandbox, necesitas ambos tokens
SQUARE_PRODUCTION_TOKEN=tu_token_de_produccion

# Otras variables de entorno para tu aplicación
NEXT_PUBLIC_APP_URL=http://localhost:3000
