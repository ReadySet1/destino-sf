    "lucide-react": "^0.468.0",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "next-themes": "^0.4.3",
    "prettier": "^3.3.3",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-day-picker": "8.10.1",
