    "react-email": "^4.0.11",
    "react-hook-form": "^7.55.0",

    "react-remove-scroll": "^2.7.1",
    "resend": "^4.4.0",
    "shippo": "^2.15.0",
    "sonner": "^2.0.5",
    "square": "^42.3.0",
    "styled-components": "6",
    "tailwind-merge": "^3.3.0",
