{
  "framework": "nextjs",
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "functions": {
    "src/app/api/**/*": {
      "maxDuration": 30
    },
    "src/app/sitemap.xml/route.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/icon0.svg",
      "destination": "/src/app/icon0.svg"
    },
    {
      "source": "/icon1.png",
      "destination": "/src/app/icon1.png"
    },
    {
      "source": "/manifest.json",
      "destination": "/src/app/manifest.json"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}